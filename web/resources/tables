.ui-tabs .ui-tabs-nav li{
    float: right !important;
}

CREATE TABLE RASAS.RS_DATA
(
  RS_NO            NUMBER(38),
  RS_YEAR          VARCHAR2(4 BYTE),
  RS_CENTER        VARCHAR2(3 BYTE),
  RS_SUB_CENTER    VARCHAR2(3 BYTE),
  RS_ENTRY_DATE    DATE,
  RS_USER_ID       VARCHAR2(10 BYTE),
  RS_TAS_DOC_NO    NUMBER(38),
  RS_TAS_COD_YEAR  VARCHAR2(4 BYTE),
  RS_TAS_COD_TYPE  NUMBER,
  RS_TAS_DATE      DATE,
  RS_TAS_USER_ID   VARCHAR2(10 BYTE),
  RS_CAR_NO        VARCHAR2(10 BYTE),
  RS_CAR_NAT       VARCHAR2(3 BYTE),
  RS_CAR_WEIGHT    NUMBER(38),
  RS_CTN_NO        VARCHAR2(11 BYTE),
  RS_CTN_WEIGHT    NUMBER(38),
  RS_GROSS_WEIGHT  NUMBER(38)
)



CREATE TABLE RASAS.RS_MAIN
(
  RS_NO                  NUMBER(38)             NOT NULL,
  RS_YEAR                VARCHAR2(4 BYTE)       NOT NULL,
  RS_CENTER              VARCHAR2(3 BYTE)       NOT NULL,
  RS_CENTER_DATE         DATE,
  RS_CENTER_USER_ID      VARCHAR2(10 BYTE),
  RS_SUB_CENTER          VARCHAR2(3 BYTE),
  RS_SUB_CENTER_DATE     DATE,
  RS_SUB_CENTER_USER_ID  VARCHAR2(10 BYTE)
)


CREATE TABLE RASAS.USERS
(
  USER_ID          VARCHAR2(10 BYTE)            NOT NULL,
  USER_NAME        VARCHAR2(50 BYTE),
  USER_TYPE        VARCHAR2(1 BYTE),
  PASSWORD         VARCHAR2(32 BYTE),
  PRIVILEGE        NUMBER(38),
  ENTRY_DATE       DATE,
  LAST_LOGIN       DATE,
  USER_CENTER      VARCHAR2(3 BYTE),
  USER_SUB_CENTER  VARCHAR2(3 BYTE)
)


CREATE TABLE RASAS.VIEW_PRIVILEGE
(
  USER_ID    VARCHAR2(10 BYTE)                  NOT NULL,
  VIEW_ID    VARCHAR2(50 BYTE)                  NOT NULL,
  PRIVILEGE  VARCHAR2(6 BYTE)                   NOT NULL
)


CREATE TABLE RASAS.GROUP_MEMBERS
(
  GROUP_ID    VARCHAR2(10 BYTE),
  USER_ID     VARCHAR2(10 BYTE),
  ENTRY_DATE  DATE
)


CREATE TABLE RASAS.CENTERS
(
  CENTER_NO    VARCHAR2(3 BYTE),
  CENTER_NAME  VARCHAR2(100 BYTE)
)

CREATE TABLE RASAS.SUB_CENTERS
(
  CENTER_NO        VARCHAR2(3 BYTE),
  SUB_CENTER_NO    VARCHAR2(3 BYTE),
  SUB_CENTER_NAME  VARCHAR2(100 BYTE)
)

CREATE TABLE RASAS.VIEWS
(
  VIEW_ID    VARCHAR2(50 BYTE),
  VIEW_NAME  VARCHAR2(100 BYTE)
)